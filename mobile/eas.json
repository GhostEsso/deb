{
    "cli": {
        "version": ">= 5.9.1",
        "promptToConfigurePushNotifications": false,
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "channel": "preview",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://deb-zeta.vercel.app/api/V1"
            }
        },
        "production": {
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}