{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/V1/stories/upload",
            "dest": "src/main.ts",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "config": {
                "maxDuration": 20
            }
        },
        {
            "src": "/api/V1/users/(.*)/profile-picture",
            "dest": "src/main.ts",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "config": {
                "maxDuration": 20
            }
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        }
    ]
}